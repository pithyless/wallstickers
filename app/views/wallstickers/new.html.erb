<div id="content"> 
  <h1 id="title"><%= t 'wallstickers.new.title' %></h1>

  <div class='wallstickers_row'>
    <%= semantic_form_for @wallsticker,
                          :url => create_wallsticker_path,
                          :html => {:multipart => true} do |f| %>
      <br />
      <%= f.inputs :title %>
      <br />
      <%= f.input :source_image, :as => :file %>

      <br />
      <%= f.buttons :commit %>
    <% end %>
  </div>

</div> 

<div id="panel"> 
  <%= render 'artist' %>
</div>
